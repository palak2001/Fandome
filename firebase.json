{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/profile/**",
      "destination": "/profile.html"
    } ,
    {
      "source": "/room/**",
      "destination": "/room.html"
    } ]
  }
}